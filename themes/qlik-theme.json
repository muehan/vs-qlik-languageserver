{
    "name": "Qlik Language Server Theme",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        "editor.foreground": "#FFFFFF",
        "editor.background": "#1E1E1E"
    },
    "tokenColors": [],
    "semanticTokenScopes": {
        "keyword": [{ "foreground": "#C586C0", "fontStyle": "bold" }],
